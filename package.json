{
  "name": "o2o-fe",
  "version": "0.0.1",
  "description": "the o2o project",
  "scripts": {
    "clean": "rimraf dist",
    "copy-static": "rm -rf ./remote-dist/static && rm ./remote-dist/index.html && cp -r ./dist/* ./remote-dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "quick:build:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.quick.js --display-chunks",
    "quick:build:detail:webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.quick.js --display-chunks --display-modules",
    "build": "npm run clean && npm run build:webpack && npm run copy-static",
    "qb": "npm run clean && npm run quick:build:webpack",
    "qbd": "npm run clean && npm run quick:build:detail:webpack",
    "start": "node build/devServer.js",
    "start2": "node build/devServer2.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "antd": "1.5.0",
    "blueimp-canvas-to-blob": "^3.3.0",
    "classnames": "^2.2.5",
    "filesaver.js": "^0.2.0",
    "highcharts-custom-events": "^1.4.0",
    "highcharts-exporting": "^0.1.2",
    "highcharts-more": "^0.1.2",
    "highcharts-offline-exporting": "^0.1.2",
    "html2canvas": "^0.5.0-beta4",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "mockjs": "^1.0.1-beta2",
    "moment": "^2.13.0",
    "papaparse": "^4.1.2",
    "proj4": "^2.3.14",
    "react": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-copy-to-clipboard": "^4.2.2",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.1.0",
    "react-highcharts": "^8.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-tagcloud": "^0.5.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "wordcloud": "^1.0.5"
  },
  "main": "lib/index.js",
